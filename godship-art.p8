pico-8 cartridge // http://www.pico-8.com
version 8
__lua__

function dance()
  print("dance!")
end

function _init()

end

frame =0


frames_jump =  {22}
frames_jump_up = {3}
frames_jump_down = {4}
frames_walk ={1,6,7,8}
frames_shoot={1,2}
frames_idle ={1}

speed = 1
frame_count = 4

direction_right = true

left  = 0
right = 1 
up    = 5
butt_z= 4
function _update()
 frame = frame +1
 speed = 2
 if (btn(left)) then
   frames = frames_walk
   frame_count = 4
   direction_right = false
   position_x = position_x -(1/speed)
 elseif (btn(right)) then
   frames = frames_walk
   frame_count = 4
   direction_right = true
   position_x = position_x +(1/speed)
 else
   frames = frames_idle
   frame_count = 1
 end

 if (btn(up)
    and position_y == ground) then
 		speed_y =-1
 end
 
 if(btn(butt_z)) then
   shoot = true
 else
   shoot = false
 end
 
 if (position_y < ground) then
      frames = frames_jump_up
      frame_count = 1
 end
  
 position_y_before =  position_y
 
 -- forces and gravity
 if( position_y <= ground) then
   position_y = position_y + speed_y
   speed_y = speed_y + (0.1) 
 elseif (position_y > ground) then
   position_y = ground
   speed_y = 0
 end
 --landing
 if(position_y_before <ground 
 and position == ground) then
   frames = frames_jump_down
   frame_count = 1
 end
 
end

ground = 14*8
position_x = 7*8
position_y = ground


speed_y  = 0
function _draw()
  cls()
  mapdraw(0, 0, 0, 0, 16, 16)
  
  frame_id = flr(frame/speed)%frame_count+1
 
  spr(frames[frame_id],
     position_x,position_y, 
      1,1,
      not direction_right)  
  if(shoot) then
  spr(23,
      position_x,position_y, 
      1,1,
      not direction_right)  
  else
    spr(24,
      position_x,position_y, 
      1,1,
      not direction_right) 
  end
  print(frame_id,10,100) 
 
end
